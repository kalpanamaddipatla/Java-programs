# Java-programs
import java.util.*;
public class NCR {
  static int fact(int n)
  {
  int fact=1;
  for(int i=n;i>=1;i--)
  fact=fact*i;
  return fact;
  }
    public static void main(String[] args) {
     System.out.println("Input1:");
     int n=3;
     System.out.println("Input2:");
     int r=2;
     System.out.println("Input3:");
     double m=1000000009;
     int ncr=(int)(fact (n)/(fact (r)*fact(n-r))%m);
     System.out.println(ncr);
    }
}
