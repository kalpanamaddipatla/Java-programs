# Java-programs
public class Add_ex1{
    public static void main(String[] args) {
    int a1[][]={{10,20},{30,40}};
    int a2[][]={{50,60},{70,80}};
    int c;
     for(int i=0;i<2;i++)
     {
         for(int j=0;j<2;j++)
         {
          c=a1[i][j]+a2[i][j];

         System.out.print(c);
         }
         System.out.println( );
     }
     
    }
}
